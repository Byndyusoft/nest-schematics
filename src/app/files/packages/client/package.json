{
  "name": "<%= clientName %>",
  "version": "0.0.0-development",
  "description": "<%= clientDescription %>",
  "homepage": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>#readme",
  "bugs": {
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>/issues"
  },
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>.git"
  },
  "license": "<%= license %>",
  "author": "<%= author %>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && yarn run build:src",
    "build:src": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "proper-url-join": "^2.1.1",
    "qs": "^6.11.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "<%= dtosName %>": "workspace:*",
    "@byndyusoft/class-validator-extended": "1.2.0",
    "@byndyusoft/nest-dynamic-module": "1.0.0",
    "@byndyusoft/nest-http-client": "1.2.2",
    "@byndyusoft/nest-swagger": "6.3.0-1",
    "@byndyusoft/tsconfig": "1.2.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@types/proper-url-join": "2.1.4",
    "@types/qs": "6.9.16",
    "axios": "0.28.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "shx": "0.3.4",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "<%= dtosName %>": "workspace:*",
    "@byndyusoft/nest-http-client": "^1.2.1",
    "@nestjs/common": "^9.2.1",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=16"
  }
}
