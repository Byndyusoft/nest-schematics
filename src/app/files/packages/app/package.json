{
  "name": "<%= appName %>",
  "version": "0.0.0-development",
  "private": true,
  "description": "<%= appDescription %>",
  "homepage": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>#readme",
  "bugs": {
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>/issues"
  },
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>.git"
  },
  "license": "<%= license %>",
  "author": "<%= author %>",
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && yarn run build:src",
    "build:src": "nest build",
    "start": "nest start --exec \"yarn node -r dotenv/config\"",
    "start:prod": "yarn node ./dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts",
      "!./src/infrastructure/**/*.ts",
      "!./src/main.ts",
      "!**/*Command.ts",
      "!**/*Dto.ts",
      "!**/*Entity.ts",
      "!**/*Mapper.ts",
      "!**/*Module.ts",
      "!**/*Query.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "text",
      "@byndyusoft/sonar-coverage-reporter"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "reporters": [
      "default",
      [
        "@casualbot/jest-sonar-reporter",
        {
          "relativePaths": true,
          "outputDirectory": "coverage",
          "outputName": "test-report.xml"
        }
      ]
    ],
    "resetMocks": true,
    "roots": [
      "<rootDir>/test"
    ],
    "setupFiles": [
      "reflect-metadata"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "<%= dtosName %>": "workspace:*",
    "@byndyusoft/class-validator-extended": "1.2.0",
    "@byndyusoft/nest-opentracing": "2.4.0",
    "@byndyusoft/nest-pino": "3.1.2-1",
    "@byndyusoft/nest-swagger": "6.3.0-1",
    "@byndyusoft/pino-logger-factory": "3.1.0",
    "@digikare/nestjs-prom": "1.0.0",
    "@nestjs/axios": "0.1.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/terminus": "9.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "helmet": "6.2.0",
    "jaeger-client": "3.19.0",
    "lodash": "4.17.21",
    "pino": "8.21.0",
    "pino-http": "8.6.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "tslib": "2.8.0"
  },
  "devDependencies": {
    "<%= dtosTestingName %>": "workspace:*",
    "@byndyusoft/sonar-coverage-reporter": "1.0.0",
    "@byndyusoft/tsconfig": "1.2.0",
    "@casualbot/jest-sonar-reporter": "2.4.0",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@types/express": "4.17.21",
    "@types/jaeger-client": "3.18.7",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.12",
    "@types/node": "16.18.115",
    "@types/source-map-support": "0.5.10",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "jest": "29.7.0",
    "jest-extended": "3.2.4",
    "jest-mock-extended": "3.0.7",
    "pino-pretty": "10.3.1",
    "shx": "0.3.4",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
