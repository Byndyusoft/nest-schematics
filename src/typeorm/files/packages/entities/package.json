{
  "name": "<%= entitiesName %>",
  "version": "0.0.0-development",
  "description": "<%= entitiesDescription %>",
  "homepage": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>#readme",
  "bugs": {
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>/issues"
  },
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= githubOrganization %>/<%= repositoryName %>.git"
  },
  "license": "<%= license %>",
  "author": "<%= author %>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && yarn run build:src",
    "build:src": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "<%= dtosName %>": "workspace:*",
    "@byndyusoft/class-validator-extended": "1.2.0",
    "@byndyusoft/nest-swagger": "6.3.0-1",
    "@byndyusoft/tsconfig": "1.2.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "shx": "0.3.4",
    "typeorm": "0.3.20",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "<%= dtosName %>": "workspace:*",
    "typeorm": "^0.3.11"
  },
  "engines": {
    "node": ">=16"
  }
}
